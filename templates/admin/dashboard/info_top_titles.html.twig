<div class="row">
  <div class="x_content">
      {% include "./admin/dashboard/info_categories.html.twig" %}
      <div class="tab-content" id="myTabContent">
          <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
            {# ORDINATEUR PORTABLE #}
            <div class="row">
            {# Total #}
              <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-primary text-white">
                  <div class="icon"><i class="fas fa-laptop text-white"></i></div>
                  <div class="count text-white">
                    {{ laptops|length }}
                  </div>
                  <h3 class=" text-white">Total</h3>
                  <p class=" text-white">Ordinateur Portable global.</p>
                </div>
              </div>
            {# Attribués #}
              <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-success text-white">
                  <div class="icon"><i class="fas fa-fingerprint text-white"></i></div>
                  <div class="count text-white">
                    {{ laptopAttribs|length }}
                  </div>
                  <h3 class=" text-white">Attribution</h3>
                  <p class=" text-white">Ordinateurs Portables attribués.</p>
                </div>
              </div>
            {# En Stock #}
              <div class="animated flipInY col-lg-2 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-info text-white">
                  <div class="icon"><i class="fas fa-database text-white"></i></div>
                  <div class="count text-white">
                    {{ laptops|length - laptopAttribs|length }}
                  </div>
                  <h3 class="text-white">En Stock</h3>
                  <p>Ordinateurs Portables en stock.</p>
                </div>
              </div>
            {# Amortis #}
              <div class="animated flipInY col-lg-2 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-danger text-white">
                  <div class="icon"><i class="fas fa-skull-crossbones text-white"></i></div>
                  <div class="count text-white">
                    {{ laptopAmortis|length }}
                  </div>
                  <h3 class="text-white">Amortis</h3>
                  <p>Ordinateurs Portables amortis</p>
                </div>
              </div>
            {# En Pannes #}
              <div class="animated flipInY col-lg-2 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-warning text-dark">
                  <div class="icon"><i class="fas fa-exclamation-triangle text-dark"></i></div>
                  <div class="count text-dark">
                    {{ laptopEnPannes|length }}
                  </div>
                  <h3 class="text-dark">En Pannes</h3>
                  <p>Ordinateur Portable en panne.</p>
                </div>
              </div>
            {# Stats #}
            
              <div class="animated flipInY col-lg-6 col-md-6 col-sm-6 ">
                <div class="tile-stats text-dark">
                  {{render_chart(chartLY)}}
                </div>
              </div>
              <div class="animated flipInY col-lg-6 col-md-6 col-sm-6 ">
                <div class="tile-stats text-dark">
                  {{render_chart(chartSM)}}
                </div>
              </div>
            </div> 
          </div>
          <div class="tab-pane fade" id="statut" role="tabpanel" aria-labelledby="statut-tab">
            {# ORDINATEUR FIXE & ALL-IN-ONE #}
            <div class="row">
            {# Total #}
              <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-dark ">
                  <div class="icon"><i class="fas fa-desktop text-white"></i></div>
                  <div class="count text-white">
                    {{ desktops|length + aios|length }}
                  </div>
                  <h3 class=" text-white">Total</h3>
                  <p class="text-white">Ordinateur Fixe global</p>
                </div>
              </div>
            {# Attribués #}
              <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-success text-white">
                  <div class="icon"><i class="fas fa-fingerprint text-white"></i></div>
                  <div class="count text-white">
                    {{ desktopAttribs|length + aioAttribs|length }}
                  </div>
                  <h3 class="text-white">Attribués</h3>
                  <p class="text-white">Ordinateurs Fixes attribués.</p>
                </div>
              </div>
            {# En stock #}
              <div class="animated flipInY col-lg-2 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-info text-white">
                  <div class="icon"><i class="fas fa-database text-white"></i></div>
                  <div class="count text-white">
                    {{ desktops|length + aios|length - desktopAttribs|length - aioAttribs|length }}
                  </div>
                  <h3 class="text-white">En Stock</h3>
                  <p class="text-white">Ordinateurs Fixes en stock.</p>
                </div>
              </div>
            {# Amortis #}
              <div class="animated flipInY col-lg-2 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-danger text-white">
                  <div class="icon"><i class="fas fa-skull-crossbones text-white"></i></div>
                  <div class="count text-white">
                    {{ desktopAmortis|length + aioAmortis|length }}
                  </div>
                  <h3 class="text-white">Amortis</h3>
                  <p>Ordinateurs Fixes amortis</p>
                </div>
              </div>
            {# En Pannes #}
              <div class="animated flipInY col-lg-2 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-warning text-dark">
                  <div class="icon"><i class="fas fa-exclamation-triangle text-dark"></i></div>
                  <div class="count text-dark">
                    {{ desktopEnPannes|length + aioEnPannes|length }}
                  </div>
                  <h3 class="text-dark">En Pannes</h3>
                  <p>Ordinateurs Fixex en pannes </p>
                </div>
              </div>
              {# Stats #}
              
                <div class="animated flipInY col-lg-6 col-md-6 col-sm-6 ">
                  <div class="tile-stats text-dark">
                    {{render_chart(chartDY)}}
                  </div>
                </div>
                <div class="animated flipInY col-lg-6 col-md-6 col-sm-6 ">
                  <div class="tile-stats text-dark">
                    {{render_chart(chartSM)}}
                  </div>
                </div>
            </div>
          </div>
          <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
            {# IMPRIMANTES COULEUR & NB , PHOTOCOPIEUSES #}
            <div class="row">
            {# Total #}
              <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-secondary ">
                  <div class="icon"><i class="fas fa-print text-white"></i></div>
                  <div class="count text-white">
                    {{ printerColors|length + printerNBs|length + photocopieuses|length }}
                  </div>
                  <h3 class=" text-white">Total</h3>
                  <p class="text-white"> Imprimantes & Photocopieuses globales</p>
                </div>
              </div>
            {# Attribués #}
              <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-success text-white">
                  <div class="icon"><i class="fas fa-fingerprint text-white"></i></div>
                  <div class="count text-white">
                    {{ printerColorAttribs|length + printerNBAttribs|length + photocopieuseAttribs|length }}
                  </div>
                  <h3 class="text-white">Attribués</h3>
                  <p class="text-white">Imprimantes & Photocopieuses attribuées.</p>
                </div>
              </div>
            {# En stock #}
              <div class="animated flipInY col-lg-2 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-info text-white">
                  <div class="icon"><i class="fas fa-database text-white"></i></div>
                  <div class="count text-white">
                    {{ printerColors|length + printerNBs|length + photocopieuses|length - printerColorAttribs|length - printerNBAttribs|length - photocopieuseAttribs|length }}
                  </div>
                  <h3 class="text-white">En Stock</h3>
                  <p class="text-white">Imprimantes & Photocopieuses en stock.</p>
                </div>
              </div>
            {# Amortis #}
              <div class="animated flipInY col-lg-2 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-danger text-white">
                  <div class="icon"><i class="fas fa-skull-crossbones text-white"></i></div>
                  <div class="count text-white">
                    {{ printerColorAmortis|length + printerNBAmortis|length + photocopieuseAmortis|length }}
                  </div>
                  <h3 class="text-white">Amortis</h3>
                  <p>Imprimantes et Photocopieuse Amorties.</p>
                </div>
              </div>
            {# En Pannes #}
              <div class="animated flipInY col-lg-2 col-md-3 col-sm-6 ">
                <div class="tile-stats bg-warning text-dark">
                  <div class="icon"><i class="fas fa-exclamation-triangle text-dark"></i></div>
                  <div class="count text-dark">
                    {{ printerColorEnPannes|length + printerNBEnPannes|length + photocopieuseEnPannes|length }}
                  </div>
                  <h3 class="text-dark">En Pannes</h3>
                  <p>Imprimantes et photocopieuse en panne</p>
                </div>
              </div>
            </div>
          </div>
      </div>
  </div>
</div>
  


